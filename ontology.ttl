        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Collection
                                    :Supplement
                                  )
                    ] ;
        rdfs:range :Person ;
        rdfs:comment "is the editor of a collection or supplement"@en ;
